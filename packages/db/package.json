{
  "name": "@sebas-chan/db",
  "version": "0.0.1",
  "description": "LanceDB データベース層",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build --force --verbose",
    "dev": "tsx watch src/index.ts",
    "test": "NODE_OPTIONS='--no-warnings' vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf dist",
    "postinstall": "npm run setup-python || true",
    "setup-python": "if command -v uv >/dev/null 2>&1; then uv venv && uv sync; elif [ -f pyproject.toml ]; then pip install -e .; fi",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sebas-chan/shared-types": "*",
    "axios": "^1.11.0",
    "nanoid": "^5.1.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
