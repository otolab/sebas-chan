{
  "name": "@sebas-chan/reporters",
  "version": "0.1.0",
  "description": "Event collection and reporting system for sebas-chan",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "dev:api": "tsx watch src/api/main.ts",
    "start:api": "node dist/api/main.js",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:contract": "vitest run tests/contract",
    "test:e2e": "vitest run tests/e2e",
    "test:perf": "vitest run tests/perf",
    "test:api": "tsx src/api/test-setup.ts",
    "cli": "tsx src/cli/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "reporters": "./dist/cli/index.js"
  },
  "keywords": [
    "sebas-chan",
    "reporter",
    "event",
    "monitoring"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.3.0",
    "@sebas-chan/reporter-sdk": "*",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "dotenv": "^17.2.2",
    "fastify": "^4.25.0",
    "inquirer": "^8.2.7",
    "node-fetch": "^2.7.0",
    "p-retry": "^5.1.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/inquirer": "^8.2.12",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
