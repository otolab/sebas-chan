{
  "name": "@sebas-chan/shared-types",
  "version": "0.0.1",
  "description": "共有型定義",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build --force --verbose",
    "dev": "tsc --watch",
    "test": "NODE_OPTIONS='--no-warnings' vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
